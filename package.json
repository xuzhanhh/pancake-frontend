{
  "name": "pancake-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "bmp:dev": "NODE_ENV=development bmp dev",
    "bmp:build:qa": "API_HOST=qa bmp build",
    "bmp:build:prod": "API_HOST=prod bmp build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:config": "jest src/__tests__/config/ --testPathIgnorePatterns",
    "integration-test": "start-server-and-test start http://localhost:3000 'cypress run'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "format:check:staged": "pretty-quick --staged --check",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "updateLPsAPR": "NODE_PATH=./src ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/updateLPsAPR.ts",
    "prepare": "husky install",
    "postinstall": "yarn run typechain",
    "typechain": "typechain --out-dir src/config/abi/types --target=ethers-v5 \"src/config/abi/*.json\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
